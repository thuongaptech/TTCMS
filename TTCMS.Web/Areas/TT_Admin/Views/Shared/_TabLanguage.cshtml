@model TTCMS.Web.Areas.TT_Admin.Models.LanguageItemViewModel[]
@if (Model.Count() > 0)
{
    int key = Model.Count();
    int icount = 1;
    foreach (var item in Model)
    {
        <img class="flag @item.Img_Icon" alt="@item.Name" src="/Content/admin/img/blank.gif"><text>@(key > icount ? " | ":"")</text>
        icount++;
    }
}
