@model TTCMS.Web.Areas.TT_Admin.Models.LogViewModel
<style>
    .pop-logdetai .form-group {
        margin-bottom:2px;
    }
</style>
<div id="modal-form" class="modal pop-logdetai" tabindex="-1">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="blue bigger">@TTCMS.Resources.Resources.LogDetailText</h4>
            </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-xs-12 col-sm-6 change-password">
                            <div class="space-4"></div>
                            <div class="form-group">
                                <label class="col-sm-5 control-label">@TTCMS.Resources.Resources.User:</label>
                                <div class="col-sm-7">
                                    @Model.UserUserName
                                </div>
                            </div>
                            <div class="space-4"></div>
                            <div class="form-group">
                                <label class="col-sm-5 control-label">@TTCMS.Resources.Resources.LinkText:</label>
                                <div class="col-sm-7">
                                    @(Model.ServiceName)/@(Model.MethodName)
                                </div>
                            </div>
                            <div class="space-4"></div>
                            <div class="form-group">
                                <label class="col-sm-5 control-label">@TTCMS.Resources.Resources.IPAddress:</label>
                                <div class="col-sm-7">
                                    @Model.ClientIpAddress
                                </div>
                            </div>
                            <div class="space-4"></div>
                            <div class="form-group">
                                <label class="col-sm-5 control-label">@TTCMS.Resources.Resources.ClientNameText:</label>
                                <div class="col-sm-7">
                                    @Model.ClientName
                                </div>
                            </div>
                        </div>
                        <div class="col-xs-12 col-sm-6 change-password">
                            <div class="space-4"></div>
                            <div class="form-group">
                                <label class="col-sm-5 control-label">@TTCMS.Resources.Resources.BrowserInfoText:</label>
                                <div class="col-sm-7">
                                    @Model.BrowserInfo
                                </div>
                            </div>
                            <div class="space-4"></div>
                            <div class="form-group">
                                <label class="col-sm-5 control-label">@TTCMS.Resources.Resources.ExecutionTimeText:</label>
                                <div class="col-sm-7">
                                   @Model.ExecutionTime
                                </div>
                            </div>
                            <div class="space-4"></div>
                            <div class="form-group">
                                <label class="col-sm-5 control-label">@TTCMS.Resources.Resources.ExecutionDurationText:</label>
                                <div class="col-sm-7">
                                    @Model.ExecutionDuration ms
                                </div>
                            </div>
                        </div>
                        <div class="col-xs-12 col-sm-12 change-password">
                            <div class="space-4"></div>
                            <div class="form-group">
                                <label class="col-sm-12 control-label">@TTCMS.Resources.Resources.ParametersText</label>
                                <div class="col-sm-12">
                                    <textarea style="width:100%">@Model.Parameters</textarea>
                                </div>
                            </div>
                      </div>
                        </div>
                </div>

                <div class="modal-footer">
                    <button class="btn btn-sm" data-dismiss="modal">
                        <i class="ace-icon fa fa-times"></i>
                        @TTCMS.Resources.Resources.ColseText
                    </button>
                </div>
        </div>
    </div>
</div>
<!-- PAGE CONTENT ENDS -->
